{
    "name": "4mindable/epigeon-sdk-php",
    "description": "ePigeon's PHP SDK for REST APIs",
    "keywords": ["ePigeon", "email marketing", "rest", "sdk"],
    "type": "library",
    "license": "GPL-3.0-or-later",
    "homepage": "https://github.com/4mindable/ePigeon-SDK-PHP/",
    "authors": [
        {
            "name": "4mindable",
            "homepage": "https://github.com/4mindable/ePigeon-SDK-PHP/contributors"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-curl": "*",
        "ext-mbstring": "*",
        "ext-zlib": "*",
        "ext-fileinfo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10"
    },
    "autoload": {
        "psr-4": {
            "Epigeon\\" : "lib/",
            "EpigeonTest\\" : "tests/"
        },
        "classmap": ["lib"]
    }
}
